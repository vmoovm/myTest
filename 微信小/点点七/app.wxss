.zshangchuan { border-bottom: 1px #ebebeb solid; border-top:1px #ebebeb solid; background: #fff;}


/*多行文本输入*/
.zsh_textarea {}
.zsh_contenteditable ,.zsh_contenteditable1 { width: 100%;}
.zsh_contenteditable textarea,.zsh_contenteditable1  textarea {-webkit-appearance: none; width: 100%; height: 140rpx; padding: 10rpx 30rpx 0; -webkit-box-sizing: border-box; box-sizing: border-box; border: none; border-radius: 0; outline: none; background: #fff; line-height: 58rpx; font-size: 32rpx; color: #333;}
.zsh_contenteditable1  textarea { border: none; border-radius: 0; outline: none; background: #fff; line-height: 1.4;}
.zsh_contenteditable1  textarea::-webkit-input-placeholder { font-size: 30rpx; }

.zsh_img,.zsh_img1 { padding:0 30rpx;}
.zsh_img1 { border-bottom: 1px #ebebeb solid;}
